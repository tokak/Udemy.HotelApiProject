@model List<ResultServiceDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}


<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Servis Başlığı</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.ServiceID</th>
            <td>@item.Title</td>
            <td><a href="/Service/DeleteService/@item.ServiceID/" class="btn btn-outline-danger">SİL</a></td>
            <td><a href="/Service/UpdateService/@item.ServiceID/" class="btn btn-outline-success">GÜNCELLE</a></td>
        </tr>

    }
</table>

<a href="/Service/AddService/" class="btn btn-info">Yeni Hizmet Girişi</a>

